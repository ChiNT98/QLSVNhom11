    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h2>Xóa Thông Tin Khoa</h2>


<div>
    <h4>Khoa</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Mã Khoa
        </dt>

        <dd>
            @Html.DisplayFor(model => model.MaKhoa)
        </dd>
        <dt>
            Tên Khoa
        </dt>

