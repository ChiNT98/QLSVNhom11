@model IEnumerable<QUANLYSINHVIEN.Models.DiemThi>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h2>Thông tin điểm thi</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Mã Sinh Viên
        </th>
        <th>
            Tên Sinh Viên
        </th>
        <th>
            Mã Môn Học
        </th>
        <th>
            Tên Môn Học
        </th>
        <th>
            Điểm Thi
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.MaSv)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TenSv)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.MaMh)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TenMh)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Diem)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.MaSv }) |
        @Html.ActionLink("Details", "Details", new { id = item.MaSv }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.MaSv })
    </td>
</tr>
}

</table>
